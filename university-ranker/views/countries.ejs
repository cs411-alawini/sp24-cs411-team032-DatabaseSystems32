<!-- Country Detail View -->
<!-- In views/countries.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Country Information</title>
    <link rel="stylesheet" href="/css/style.css">
</head>
<body>
    <h1>Country Information: <%= country.Name %></h1>
    <p>Living Cost Score: <%= country.LivingCostScore %></p>
    <p>Safety Score: <%= country.SafetyScore %></p>
    <p>Rights Score: <%= country.RightsScore %></p>
    <p>Climate Score: <%= country.ClimateScore %></p>
    <a href="/">Back to Universities</a>
</body>
</html>
